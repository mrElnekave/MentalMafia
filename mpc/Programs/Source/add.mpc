from Compiler.types import sint
from Compiler.library import *

def main():
    # Get secret input from each player (assuming 2 players)
    inputs = [sint.get_input_from(i) for i in range(2)]

    # Sum all inputs
    total = sum(inputs)

    # Reveal the result to player 0
    print_ln("Sum of inputs: %s", total.reveal())


main()
